{
  "name": "0000-fsams-redvalley-clinicalplatform",
  "private": true,
  "version": "0.1.0",
  "description": "Red Valley Technical Test",
  "main": "index.js",
  "scripts": {
    "dev:api": "nx run api:serve",
    "dev:web": "nx run web:dev",
    "dev:all": "nx run-many --target=serve --projects=api,web --parallel",
    "compose:up": "docker compose up -d",
    "compose:down": "docker compose down",
    "db:migrate": "npm exec --workspace api sequelize-cli db:migrate",
    "db:seed": "npm exec --workspace api sequelize-cli db:seed:all",
    "db:reset": "npm run compose:down && npm run compose:up && npm run db:migrate && npm run db:seed",
    "build:api": "nx run api:build",
    "build:web": "nx run web:build",
    "build:all": "nx run-many --target=build --all",
    "lint": "nx run-many --target=lint --all",
    "format": "nx format:write",
    "graph": "nx graph",
    "test": "nx run-many --target=test --all -- --coverage"
  },
  "workspaces": [
    "api"
  ],
